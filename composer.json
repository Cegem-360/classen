{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "automattic/woocommerce": "^3.1",
        "bensampo/laravel-enum": "^6.12",
        "filament/filament": "^4.0",
        "glebsky/laravel-lang-generator": "^1.0.1",
        "guzzlehttp/guzzle": "^7.9.2",
        "laravel/framework": "*",
        "laravel/sanctum": ">=4.0.8",
        "laravel/telescope": "^5.5",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": ">=3.6",
        "maatwebsite/excel": "^3.1.64",
        "masmerise/livewire-toaster": ">=2.7",
        "nunomaduro/essentials": "@dev",
        "ralphjsmit/laravel-seo": "^1.7",
        "spatie/laravel-googletagmanager": "^2.7.1",
        "spatie/laravel-honeypot": "^4.6",
        "spatie/laravel-sitemap": "^7.3.5",
        "whitecube/laravel-cookie-consent": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15.2",
        "barryvdh/laravel-ide-helper": "^3.5.5",
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^1.24.1",
        "filament/upgrade": "^4.0",
        "larastan/larastan": "^3.4.0",
        "laravel/boost": "^1.1",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.22.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8",
        "peckphp/peck": "^0.1.3",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-laravel": "*",
        "pestphp/pest-plugin-type-coverage": "*",
        "rector/rector": "^2.0.14"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@composer ide-helper"
        ],
        "pint": "vendor/bin/pint",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N --reset"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "lint": [
            "pint",
            "npm run lint"
        ],
        "refactor": "rector",
        "test:type-coverage": "pest --type-coverage --min=100",
        "test:typos": "peck",
        "test:lint": [
            "pint --test",
            "npm run test:lint"
        ],
        "test:unit": "pest --parallel --coverage --exactly=100.0",
        "test:types": "phpstan",
        "test:refactor": "rector --dry-run",
        "test": [
            "@test:type-coverage",
            "@test:typos",
            "@test:unit",
            "@test:lint",
            "@test:types",
            "@test:refactor"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
